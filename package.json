{
  "name": "vscode-fuzzy-search",
  "displayName": "VS Code Fuzzy Search",
  "description": "在 VS Code 中进行模糊搜索",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.fuzzySearch"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.fuzzySearch",
        "title": "Fuzzy Search"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.3.5",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "fuzzysort": "^3.0.2"
  }
}
