{
  "name": "fuzzy-search-inone",
  "displayName": "VS Code Fuzzy Search",
  "description": "在 VS Code 中进行模糊搜索",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.9.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:fuzzySearchInOne.searchActiveEditor"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fuzzySearchInOne.searchActiveEditor",
        "title": "Fuzzy Search In One: Search Active Editor"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/vscode": "^1.90.0",
    "typescript": "^4.3.5",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "fuzzysort": "^3.0.2"
  }
}
